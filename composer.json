{
    "name": "addgod/nova-translate-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-translatable": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Addgod\\NovaTranslateField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Addgod\\NovaTranslateField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
